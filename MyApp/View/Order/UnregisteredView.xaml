<?xml version="1.0" encoding="utf-8" ?><ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"             xmlns:editors="clr-namespace:Syncfusion.Maui.Inputs;assembly=Syncfusion.Maui.Inputs"             x:Class="MyApp.UnregisteredView"                          xmlns:viewmodel="clr-namespace:MyApp.ViewModel"Shell.TabBarIsVisible="False"x:DataType="viewmodel:VerificationViewModel"             Title="Stock Audit" BackgroundColor="White">    <ContentPage.ToolbarItems>        <!-- Define a custom toolbar item with the "+" icon -->        <ToolbarItem  IconImageSource="notifications_none.png"  Priority="0"  Order="Primary"     Clicked="ToolbarItem_Clicked"  />    </ContentPage.ToolbarItems>    <Shell.BackButtonBehavior>        <BackButtonBehavior Command="{Binding BackCommand}"                   IsEnabled="False"  />    </Shell.BackButtonBehavior>    <Grid RowDefinitions="*,auto">        <VerticalStackLayout  Grid.Row="0">            <Frame Padding="0" HeightRequest="56" WidthRequest="343" CornerRadius="4" Margin="0,15,0,0"  BorderColor="#6F7978">                <Entry Placeholder="Enter Locations" FontSize="16" Text="{Binding Location}" TextColor="#3F4948" PlaceholderColor="#3F4948" VerticalOptions="Center" Margin="13,0,0,0" />            </Frame>            <Label Margin="25,0,0,0" Text="{Binding LocationErrors}" TextColor="Red" />            <Frame Padding="0" HeightRequest="56" WidthRequest="343" CornerRadius="4" Margin="0,0,0,0" BorderColor="#6F7978">                <Grid >                    <editors:SfComboBox IsClearButtonVisible="False" x:Name="comboBox" HeightRequest="56" BackgroundColor="Transparent"   Placeholder="Purpose" FontSize="16" PlaceholderColor="#3F4948" Stroke="Transparent" TextColor="#3F4948" VerticalOptions="Center" Margin="5,0,10,0" ItemsSource="{Binding Purpose}" SelectedItem="{Binding SelectedPurpose}" />                    <!--<Picker x:Name="PurposeName" Grid.Column="0" Title="Purpose" FontSize="16" TitleColor="#3F4948" TextColor="#3F4948" VerticalOptions="Center"  Margin="13,0,0,0"ItemsSource="{Binding Purpose}"  SelectedItem="{Binding SelectedPurpose}" />-->                    <!--<ImageButton Source="dropdown.png" Grid.Column="1"  HorizontalOptions="Center" VerticalOptions="Center" WidthRequest="10" HeightRequest="10"  >                    </ImageButton>-->                </Grid>            </Frame>            <Label Margin="25,0,0,0" Text="{Binding PurposeErrors}" TextColor="Red" />            <Frame Padding="0" HeightRequest="56" WidthRequest="343" CornerRadius="4" Margin="0,0,0,0" BorderColor="#6F7978">                <Grid >                    <Grid  ColumnDefinitions="*, auto">                        <Entry Placeholder="Chosse Date" FontSize="16" ZIndex="0" Margin="15,5,0,0" PlaceholderColor="#3F4948"/>                        <DatePicker  Margin="13,0,0,0" FontSize="16" ZIndex="1" Opacity="0" WidthRequest="343" TextColor="#6F7978" Date="{Binding ChoseDate}"   VerticalOptions="Center" />                        <ImageButton Source="calender.png" Grid.Column="1" ZIndex="0"  HorizontalOptions="Center" VerticalOptions="Center" WidthRequest="24" HeightRequest="24"  Margin="0,0,20,0" >                        </ImageButton>                    </Grid>                </Grid>            </Frame>            <Label Margin="25,0,0,0" Text="{Binding DateErrors}" TextColor="Red" />        </VerticalStackLayout>        <Button Command="{Binding SubmitCommand}" Style="{StaticResource LargeButtonColor}" Grid.Row="1" Text="Schedule" VerticalOptions="End"  HorizontalOptions="Center" Margin="0,0,0,10"/>    </Grid></ContentPage> 